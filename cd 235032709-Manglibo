[32m+[m[32m@echo off[m[41m[m
[32m+[m[32msetlocal enabledelayedexpansion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m:goBack[m[41m[m
[32m+[m[32mecho Enter the source directory:[m[41m[m
[32m+[m[32mset /p "dir=Source Directory: "[m[41m[m
[32m+[m[32mif not exist "!dir!" ([m[41m[m
[32m+[m[32m    echo Folder does not exist. Please try again.[m[41m[m
[32m+[m[32m    goto :goBack[m[41m[m
[32m+[m[32m)[m[41m[m
[32m+[m[41m[m
[32m+[m[32m:goBackDest[m[41m[m
[32m+[m[32mecho Enter the destination directory:[m[41m[m
[32m+[m[32mset /p "dest=Destination Directory: "[m[41m[m
[32m+[m[32mif not exist "!dest!" ([m[41m[m
[32m+[m[32m    echo Folder does not exist. Please try again.[m[41m[m
[32m+[m[32m    goto :goBackDest[m[41m[m
[32m+[m[32m)[m[41m[m
[32m+[m[41m[m
[32m+[m[32mset "archive=!dest!\archive"[m[41m[m
[32m+[m[32mif not exist "!archive!" ([m[41m[m
[32m+[m[32m    mkdir "!archive!"[m[41m[m
[32m+[m[32m)[m[41m[m
[32m+[m[41m[m
[32m+[m[32mcd /d "!dir!"[m[41m[m
[32m+[m[41m[m
[32m+[m[32mforfiles /m *.txt /d -365 /c "cmd /c move @file "!archive!""[m[41m[m
[32m+[m[41m[m
[32m+[m[32mecho Here are the text files sorted by date/time:[m[41m[m
[32m+[m[32mfor /f "delims=" %%i in ('dir /b /o:d *.txt') do ([m[41m[m
[32m+[m[32m    echo %%i[m[41m[m
[32m+[m[32m)[m[41m[m
[32m+[m[41m[m
[32m+[m[32m:fileLoop[m[41m[m
[32m+[m[32mdir[m[41m[m
[32m+[m[41m[m
[32m+[m[32mecho Enter the file from "!dir!" to move to "!dest!" (or type 'done' to finish):[m[41m [m
[32m+[m[32mset /p "file="[m[41m[m
[32m+[m[41m[m
[32m+[m[32mif /I "!file!"=="done" ([m[41m[m
[32m+[m[32m    echo Done.[m[41m[m
[32m+[m[32m    goto :end[m[41m[m
[32m+[m[32m)[m[41m[m
[32m+[m[32mif not exist "!file!" ([m[41m[m
[32m+[m[32m    cls[m[41m[m
[32m+[m[32m    echo File does not exist. Please try again.[m[41m[m
[32m+[m[32m    goto :fileLoop[m[41m[m
[32m+[m[32m)[m[41m[m
[32m+[m[41m[m
[32m+[m[32mmove "!file!" "!dest!\archive"[m[41m[m
[32m+[m[32mgoto :fileLoop[m[41m[m
[32m+[m[41m[m
[32m+[m[32m:end[m[41m[m
[32m+[m[32mecho Here are the files in the archive directory sorted by size:[m[41m[m
[32m+[m[32mfor /f "tokens=*" %%A in ('dir /b /s /o:s "!archive!"') do ([m[41m[m
[32m+[m[32m    echo %%~nA %%~pA[m[41m[m
[32m+[m[32m)[m[41m[m
[32m+[m[41m[m
[32m+[m[32m:deleteLoop[m[41m[m
[32m+[m[32mecho Do you want to delete the old, large files? (Files larger than 1MB will be deleted.) (yes/no/finish/repeat):[m[41m[m
[32m+[m[32mset /p "delete= "[m[41m[m
[32m+[m[41m[m
[32m+[m[32mif /I "!delete!"=="yes" ([m[41m[m
[32m+[m[32m    for /R "!dest!\archive" %%F in (*.txt) do ([m[41m[m
[32m+[m[32m        if %%~zF GTR 1000000 ([m[41m[m
[32m+[m[32m            del /P "%%F"[m[41m[m
[32m+[m[32m        )[m[41m[m
[32m+[m[32m    )[m[41m[m
[32m+[m[32m    goto :deleteLoop[m[41m[m
[32m+[m[32m) else if /I "!delete!"=="no" ([m[41m[m
[32m+[m[32m    echo Files will not be deleted.[m[41m[m
[32m+[m[32m    goto :deleteLoop[m[41m[m
[32m+[m[32m) else if /I "!delete!"=="finish" ([m[41m[m
[32m+[m[32m    echo Selected "finish".[m[41m[m
[32m+[m[32m    goto :finally[m[41m[m
[32m+[m[32m) else if /I "!delete!"=="repeat" ([m[41m[m
[32m+[m[32m    echo Going back.[m[41m[m
[32m+[m[32m    goto :goBack[m[41m[m
[32m+[m[32m) else ([m[41m[m
[32m+[m[32m    echo Invalid input. Please try again.[m[41m[m
[32m+[m[32m    goto :deleteLoop[m[41m[m
[32m+[m[32m)[m[41m[m
[32m+[m[41m[m
[32m+[m[32m:finally[m[41m[m
[32m+[m[32mecho Done![m[41m[m
[32m+[m[32mpause[m[41m[m

