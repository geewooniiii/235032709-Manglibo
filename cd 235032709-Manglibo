[32m+[m[32mcls[m[41m[m
[32m+[m[32mecho.[m[41m[m
[32m+[m[32mecho Select a shape:[m[41m[m
[32m+[m[32mecho 1. Circle[m[41m[m
[32m+[m[32mecho 2. Triangle[m[41m[m
[32m+[m[32mecho 3. Quadrilateral[m[41m[m
[32m+[m[32mecho 4. Exit[m[41m[m
[32m+[m[32mset /p shape="Enter your choice (1, 2, 3, 4): "[m[41m[m
[32m+[m[41m[m
[32m+[m[32mif "%shape%"=="1" goto circle[m[41m[m
[32m+[m[32mif "%shape%"=="2" goto triangle[m[41m[m
[32m+[m[32mif "%shape%"=="3" goto quadrilateral[m[41m[m
[32m+[m[32mif "%shape%"=="4" goto exit[m[41m[m
[32m+[m[32mecho Invalid! Try again.[m[41m[m
[32m+[m[32mgoto menu[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m:circle[m[41m[m
[32m+[m[32mset /p radius="Enter the radius of the circle: "[m[41m[m
[32m+[m[32mset /a area=314 * %radius% * %radius% / 100[m[41m[m
[32m+[m[32mecho The area of the circle is %area%[m[41m[m
[32m+[m[32mpause[m[41m[m
[32m+[m[32mgoto menu[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m:triangle[m[41m[m
[32m+[m[32mset /p a="Enter the length of side a: "[m[41m[m
[32m+[m[32mset /p b="Enter the length of side b: "[m[41m[m
[32m+[m[32mset /p c="Enter the length of side c: "[m[41m[m
[32m+[m[41m[m
[32m+[m[32mset /a s=(%a% + %b% + %c%) / 2[m[41m[m
[32m+[m[32mset /a area=s*(s-%a%)*(s-%b%)*(s-%c%)[m[41m[m
[32m+[m[32mset /a area=%area% ** 0.5[m[41m[m
[32m+[m[41m[m
[32m+[m[32mif %a%==%b% if %b%==%c% ([m[41m[m
[32m+[m[32m    echo The triangle is equilateral.[m[41m[m
[32m+[m[32m    goto triangle_end[m[41m[m
[32m+[m[32m)[m[41m[m
[32m+[m[41m[m
[32m+[m[32mif %a%==%b% ([m[41m[m
[32m+[m[32m    echo The triangle is isosceles.[m[41m[m
[32m+[m[32m    goto triangle_end[m[41m[m
[32m+[m[32m)[m[41m[m
[32m+[m[32mif %a%==%c% ([m[41m[m
[32m+[m[32m    echo The triangle is isosceles.[m[41m[m
[32m+[m[32m    goto triangle_end[m[41m[m
[32m+[m[32m)[m[41m[m
[32m+[m[32mif %b%==%c% ([m[41m[m
[32m+[m[32m    echo The triangle is isosceles.[m[41m[m
[32m+[m[32m    goto triangle_end[m[41m[m
[32m+[m[32m)[m[41m[m
[32m+[m[41m[m
[32m+[m[32mecho The triangle is scalene.[m[41m[m
[32m+[m[41m[m
[32m+[m[32m:triangle_end[m[41m[m
[32m+[m[32mecho The area of the triangle is %area%[m[41m[m
[32m+[m[32mpause[m[41m[m
[32m+[m[32mgoto menu[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m:quadrilateral[m[41m[m
[32m+[m[32mset /p length="Enter the length: "[m[41m[m
[32m+[m[32mset /p width="Enter the width: "[m[41m[m
[32m+[m[32mset /a area=%length% * %width%[m[41m[m
[32m+[m[32mecho The area of the quadrilateral is %area%[m[41m[m
[32m+[m[41m[m
[32m+[m[32mif %length%==%width% ([m[41m[m
[32m+[m[32m    echo The quadrilateral is a square.[m[41m[m
[32m+[m[32m) else ([m[41m[m
[32m+[m[32m    echo The quadrilateral is a rectangle.[m[41m[m
[32m+[m[32m)[m[41m[m
[32m+[m[32mpause[m[41m[m
[32m+[m[32mgoto menu[m[41m[m
[32m+[m[41m[m
[32m+[m[32m:exit[m[41m[m
[32m+[m[32mpause[m[41m[m
[32m+[m[32mexit[m[41m[m

